{
  "files": ["src/.swrc.ts"],
  "compilerOptions": {
    "target": "esnext",
    "module": "nodenext",
    "moduleResolution": "nodenext",

    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,

    "allowJs": false,
    "strict": true,
    "alwaysStrict": true,
    "strictFunctionTypes": false,
    "noUnusedLocals": true,
    "noImplicitAny": true,

    "removeComments": false,
    "sourceMap": false,
    "declaration": true,
    "skipLibCheck": true,

    "outDir": "",
    "declarationDir": ""
  }
}
